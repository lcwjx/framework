<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppDialogTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="AppProgressDialogTheme" parent="AppDialogTheme">
        <item name="android:windowBackground">@color/progress_dialog_bg</item>
    </style>

    <style name="AppExcuteDialogTheme" parent="AppDialogTheme">
        <item name="android:windowBackground">@color/excute_dialog_bg</item>
    </style>

    <style name="text_12_333333">
        <item name="android:textSize">12dp</item>
        <item name="android:textColor">#ff333333</item>
    </style>

    <style name="text_15_333333">
        <item name="android:textSize">15dip</item>
        <item name="android:textColor">#333333</item>
    </style>

    <style name="hor_cell_line">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">#ffe5e5e5</item>
    </style>
</resources>